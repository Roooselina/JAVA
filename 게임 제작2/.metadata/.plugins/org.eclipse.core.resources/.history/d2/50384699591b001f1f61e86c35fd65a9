package Monster;

import Entity.CommonEntity;
import main.GamePanel;

public class MON_GREENFAIRY extends CommonEntity{

	public MON_GREENFAIRY(GamePanel gp) {
		super(gp);
		
		name = "나무 정령";
		speed = 1;
		maxLife = 4;
		life = maxLife;
		
		solidArea.x = 44;
		solidArea.y= 55;
		solidArea.width = 45;
		solidArea.height=30;
		solidAreaDefaultX = solidArea.x;
		solidAreaDefaultY = solidArea.y;
		
	}
	public void getImage() {
		up1 = setup("/monsters/나무-")
	}
	
}
