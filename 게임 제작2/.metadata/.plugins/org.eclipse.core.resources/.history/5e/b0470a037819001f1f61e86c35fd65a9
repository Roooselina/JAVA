package main;

import Object.OBJ_BOX_LEFT;

public class ObjectSetter {
	GamePanel gp;
	
	public ObjectSetter(GamePanel gp) {
		this.gp = gp;
	}
	
	public void setObject() {
		gp.obj[0] = new OBJ_BOX_LEFT();
		gp.obj[0].worldX = 3 * gp.getOrigianlCharacterSize();
		gp.obj[0].worldY = 1 * gp.getOrigianlCharacterSize();
		
		gp.obj[1] = new OBJ_BOX_LEFT();
		gp.obj[1].worldX = 3 * gp.getOrigianlCharacterSize();
		gp.obj[1].worldY = 14 * gp.getOrigianlCharacterSize();
	}
}
