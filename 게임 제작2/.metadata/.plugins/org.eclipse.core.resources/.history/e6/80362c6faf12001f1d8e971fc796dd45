public class CommonEntity {
	public int x, y;
	public int speed;
	public int HP;
	public int MP;
	
	public BufferedImage st1, st2, st3, st4, right1, right2, right3, right4, left1, left2, left3, left4;
	public String direction;
	
	public int spriteCounter = 0;
	public int spriteNum = 1;
	
}
