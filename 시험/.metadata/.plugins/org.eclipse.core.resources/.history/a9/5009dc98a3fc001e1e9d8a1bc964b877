package 시험;

interface Animal{
	void walk();
	void fly();
	void sing();
}

class Bird implements Animal{
	String name;
	
	Bird(String name){
		this.name=name;
	}
	@Override
	public void walk() {
		System.out.print(name+"은 걸을 수 있음");
	}

	@Override
	public void fly() {
		System.out.print("걸을 수 있음");
		
	}

	@Override
	public void sing() {
		System.out.print("노래할 수 있음");
	}
	
}

public class test {

	public static void main(String[] args) {
		Bird c = new Bird("Bird");
		c.walk();
		c.fly();
		c.sing();
	}
}

