package main;

import Object.OBJ_Handler;

public class ObjectSetter {
	GamePanel gp;
	
	public ObjectSetter(GamePanel gp) {
		this.gp = gp;
	}
	
	public void setObject() {
		gp.obj[0] = new OBJ_Handler();
		gp.obj[0].worldX = 3 * gp.getOrigianlCharacterSize();
		gp.obj[0].worldY = 2 * gp.getOrigianlCharacterSize();
		
		gp.obj[0] = new OBJ_Handler();
		gp.obj[0].worldX = 4 * gp.getOrigianlCharacterSize();
		gp.obj[0].worldY = 14 * gp.getOrigianlCharacterSize();
	}
}
