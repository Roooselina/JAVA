package 시험;

import java.util.Iterator;

class CardDeck implements Iterator<String> {
    String[] card = {"2", "3", "4", "5", "6", "7", "8", "9", "10", "Queen", "King", "Joker", "ACE"};
    int index = 0;

    @Override
    public boolean hasNext() {
    	if (card.length>index) {
    		return true;
    	}
    	else return false;
    }

    @Override
    public String next() {
    	return card[index];
    }
    
    public void remove() {
    	card[index++]=null;
    }
}

public class Test {
    public static void main(String[] args) {
        CardDeck cardDeck = new CardDeck();
        while (cardDeck.hasNext()) {
            System.out.println(cardDeck.next());
            cardDeck.remove();
        }
    }
}
